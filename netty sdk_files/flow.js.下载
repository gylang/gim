+Shift+Y"),this.addAction("formattedText",function(){var t=m.getView().getState(m.getSelectionCell());if(null!=t){var e="1";m.stopEditing(),m.getModel().beginUpdate();try{if("1"==t.style.html){e=null;var n=m.convertValueToString(t.cell);"0"!=mxUtils.getValue(t.style,"nl2Br","1")&&(n=n.replace(/\n/g,"").replace(/<br\s*.?>/g,"\n"));var i=document.createElement("div");i.innerHTML=n,n=mxUtils.extractTextWithWhitespace(i.childNodes),m.cellLabelChanged(t.cell,n)}else{n=mxUtils.htmlEntities(m.convertValueToString(t.cell),!1);"0"!=mxUtils.getValue(t.style,"nl2Br","1")&&(n=n.replace(/\n/g,"<br/>")),m.cellLabelChanged(t.cell,m.sanitizeHtml(n))}m.setCellStyles("html",e),s.fireEvent(new mxEventObject("styleChanged","keys",["html"],"values",[null!=e?e:"0"],"cells",m.getSelectionCells()))}finally{m.getModel().endUpdate()}}}),this.addAction("wordWrap",function(){var t=m.getView().getState(m.getSelectionCell()),e="wrap";m.stopEditing(),null!=t&&"wrap"==t.style[mxConstants.STYLE_WHITE_SPACE]&&(e=null),m.setCellStyles(mxConstants.STYLE_WHITE_SPACE,e)}),this.addAction("rotation",function(){var t="0",e=m.getView().getState(m.getSelectionCell());null!=e&&(t=e.style[mxConstants.STYLE_ROTATION]||t);var n=new FilenameDialog(s,t,mxResources.get("apply"),function(t){null!=t&&0<t.length&&m.setCellStyles(mxConstants.STYLE_ROTATION,t)},mxResources.get("enterValue")+" ("+mxResources.get("rotation")+" 0-360)");s.showDialog(n.container,375,80,!0,!0),n.init()}),this.addAction("resetView",function(){m.zoomTo(1),s.resetScrollbars()},null,null,Editor.ctrlKey+"+H"),this.addAction("zoomIn",function(t){m.zoomIn()},null,null,Editor.ctrlKey+" + (Numpad) / Alt+Mousewheel"),this.addAction("zoomOut",function(t){m.zoomOut()},null,null,Editor.ctrlKey+" - (Numpad) / Alt+Mousewheel"),this.addAction("fitWindow",function(){var t=m.isSelectionEmpty()?m.getGraphBounds():m.getBoundingBox(m.getSelectionCells()),e=m.view.translate,n=m.view.scale;t.width/=n,t.height/=n,t.x=t.x/n-e.x,t.y=t.y/n-e.y;var i=m.container.client